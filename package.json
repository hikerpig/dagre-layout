{
  "name": "dagre-layout",
  "version": "0.8.8",
  "description": "Graph layout for JavaScript",
  "author": "Tyler Long <tyler4long@gmail.com>",
  "main": "dist/dagre-layout.cjs.js",
  "module": "dist/dagre-layout.js",
  "keywords": [
    "graph",
    "layout",
    "dagre"
  ],
  "scripts": {
    "format": "prettier --fix -w src",
    "jest": "jest --coverage",
    "karma": "node -r @babel/register node_modules/.bin/karma start",
    "test": "yarn jest && yarn karma --single-run",
    "bench": "node -r babel-register lib/bench.js",
    "build": "yarn rollup -c",
    "typecheck": "yarn tsc --noEmit",
    "build:watch": "yarn build --watch",
    "release": "yarn build -p",
    "upgrade": "yarn-upgrade-all",
    "prepush": "yarn test",
    "prepublishOnly": "yarn release"
  },
  "dependencies": {
    "graphlibrary": "^2.2.0",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/lodash": "^4.14.170",
    "benchmark": "^2.1.4",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "husky": "^0.14.3",
    "jest": "^27.0.4",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-safari-launcher": "^1.0.0",
    "mocha": "^5.0.4",
    "prettier": "^2.3.1",
    "rollup": "^2.52.1",
    "sprintf": "^0.1.5",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4",
    "yarn-upgrade-all": "^0.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tylingsoft/dagre-layout.git"
  },
  "license": "MIT",
  "files": [
    "dist/",
    "index.js"
  ],
  "jest": {
    "testRegex": "test/.+?-test\\.js",
    "testPathIgnorePatterns": [
      "test/bundle-test\\.js"
    ]
  }
}
